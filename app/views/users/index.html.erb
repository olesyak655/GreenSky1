<h1>Register users</h1>
<div class="div_main">
<table>
  <tr>
    <th class="td_index">First Name
    <th class="td_index">Last Name
    <th class="td_index">E-mail
  </tr>
<% @users.each do |u| %>
  <tr> 
    <td class="td_index"><%=  u.first_name.to_s %>
    <td class="td_index"><%=  u.last_name.to_s %>
    <td class="td_index"><%=  u.email.to_s %>
  </tr>
<% end %>
</div>


  


