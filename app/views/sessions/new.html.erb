<h3>Log in</h3>

<%= form_tag sessions_path do %>

<div class="div_main">
  <p>
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
  </p>

  <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
  </p>

  <p>
      Administrator? <%= check_box_tag :is_adm %>
  </p>

  <%= submit_tag "Log in", class: "btn btn-info" %>

</div>

<% end %>

